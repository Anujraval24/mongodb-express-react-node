{
	"name": "mongodb-express-react-node",
	"version": "1.0.0",
	"main": "server.js",
	"repository": "https://github.com/Anujraval24/mongodb-express-react-node.git",
	"author": "Anuj Raval <anujraval24@gmail.com>",
	"license": "MIT",
	"scripts": {
		"start": "npm run build && npm run prod",
		"prod": "nodemon --exec babel-node server.js",
		"client": "cd client && npm run dev",
		"server": "nodemon --exec babel-node server.js",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"build": "babel src --out-dir dist"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.5",
		"@babel/core": "^7.14.6",
		"@babel/node": "^7.14.7",
		"@babel/preset-env": "^7.14.7",
		"@babel/register": "^7.14.5",
		"autoprefixer": "^10.3.1",
		"nodemon": "^2.0.9",
		"postcss": "^8.3.6",
		"tailwindcss": "^2.2.6"
	},
	"dependencies": {
		"bcrypt": "^5.0.1",
		"chalk": "^4.1.1",
		"compression": "^1.7.4",
		"concurrently": "^6.2.0",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"helmet": "^4.6.0",
		"ip": "^1.1.5",
		"joi": "^17.4.1",
		"mongoose": "^5.13.3",
		"morgan": "^1.10.0",
		"winston": "^3.3.3"
	}
}
