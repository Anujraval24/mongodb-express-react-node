{
	"name": "mongodb-express-react-node",
	"version": "1.0.0",
	"main": "server.js",
	"repository": "https://github.com/Anujraval24/mongodb-express-react-node.git",
	"author": "Anuj Raval <anujraval24@gmail.com>",
	"license": "MIT",
	"scripts": {
		"start": "yarn build && yarn prod",
		"prod": "nodemon --exec babel-node server.js",
		"client": "cd client && yarn start",
		"server": "nodemon --exec babel-node server.js",
		"dev": "concurrently \"yarn server\" \"yarn client\"",
		"build": "babel src --out-dir dist"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.5",
		"@babel/core": "^7.14.6",
		"@babel/node": "^7.14.7",
		"@babel/preset-env": "^7.14.7",
		"@babel/register": "^7.14.5",
		"nodemon": "^2.0.9"
	},
	"dependencies": {
		"chalk": "^4.1.1",
		"compression": "^1.7.4",
		"concurrently": "^6.2.0",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"helmet": "^4.6.0",
		"ip": "^1.1.5",
		"morgan": "^1.10.0"
	}
}
